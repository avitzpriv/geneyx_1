  crossorigin="anonymous"></script>
<link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">

<section id="labs" class="container">
  <div class="container">
  <h2>Lab List</h2>
    <div class="row col-md-7 ">
      <table id="customerTable" class="table table-bordered table-hover table-responsive">
        <thead>
          <tr>
            <th>Lab Id</th>
            <th>Name</th>
            <th>Address</th>
<!--            <th>User Name</th>
            <th>User Email</th> -->
            <th>Active</th>
            <th>Deleted</th>
            <th>Type</th>
            <th>Property1</th>
            <th>Property1</th>
            <th></th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          {{#each labs}}
          <tr>
            <td><i class="fa fa-flask"></i><a href='labs/{{id}}'>   {{id}}</a></td>
            <td>{{name}}</td>
            <td>{{address}}</td>>
<!--            <td>{{user_name}}</td>>
            <td>{{user_email}}</td>>-->
            <td>{{active}}</td>
            <td>{{deleted}}</td>
            <td>{{type}}</td>
            <td>{{property1}}</td>>
            <td>{{property2}}</td>>
            <td><a href='labs/reverse/{{id}}'><i {{#if deleted}}
                                                   class="fa fa-undo">
                                                   {{else}}
                                                   class="fa fa-trash">
                                                   {{/if}}
                                                   </i></a></td>
            <td><a href='labs/edit/{{id}}'><i class="fa fa-edit"></i></a></td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>

</section>